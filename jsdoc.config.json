{

	"plugins": [
		"plugins/markdown"
	],
	"source":{
		"include":["src"]
	},
	"templates" : {
	"cleverLinks"     : false,
	"monospaceLinks"  : false,
	"default"         : {
		"outputSourceFiles" : true
	},
	"systemName"      : "JScrollView",
	"footer"          : "",
	"copyright"       : "Copyright (C) 2017 okaxaki",
	"navType"         : "vertical",
	"theme"			  : "yeti",
	"linenums"        : true,
	"collapseSymbols" : false,
	"inverseNav"      : true
	},
	"markdown" : {
		"parser": "gfm"
	},
	"opts": {
		"template": "node_modules/ink-docstrap/template",
		"encoding": "utf8",
		"destination": "./docs/",
		"recurse": true
	}
}
